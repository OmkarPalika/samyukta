{
  "name": "samyukta",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:network": "next dev -H 0.0.0.0",
    "build": "npm run update-sw-version && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "fresh-build": "npm run clean && npm run build",
    "deploy": "npm run fresh-build && vercel --prod",
    "update-sw-version": "node scripts/update-sw-version.js",
    "init-db": "npm exec tsx scripts/init-db.ts",
    "seed-db": "npm exec tsx scripts/seed-db.ts",
    "reset-db": "npm exec tsx scripts/reset-db.ts",
    "test-apis": "npm exec tsx scripts/test-apis.ts",
    "setup-db": "npm run init-db && npm run seed-db",
    "backup-db": "npm exec tsx scripts/backup-db.ts",
    "backup:create": "npm exec tsx scripts/backup-db.ts create",
    "backup:list": "npm exec tsx scripts/backup-db.ts list",
    "backup:export": "npm exec tsx scripts/backup-db.ts export",
    "backup:auto": "npm exec tsx scripts/backup-db.ts auto",
    "recovery": "npm exec tsx scripts/recovery-procedures.ts",
    "recovery:check": "npm exec tsx scripts/recovery-procedures.ts check-integrity",
    "recovery:fix-orphaned": "npm exec tsx scripts/recovery-procedures.ts fix-orphaned",
    "recovery:fix-duplicates": "npm exec tsx scripts/recovery-procedures.ts fix-duplicates",
    "recovery:emergency": "npm exec tsx scripts/recovery-procedures.ts emergency-restore",
    "recovery:health": "npm exec tsx scripts/recovery-procedures.ts health-check",
    "fix-auth": "npm exec tsx scripts/fix-auth-issues.ts",
    "add-cybersecurity-track": "npm exec tsx scripts/add-cybersecurity-track.ts",
    "seo:audit": "next build && next start & sleep 5 && lighthouse http://localhost:3000 --output=html --output-path=./seo-audit.html --chrome-flags='--headless' && pkill -f 'next start'",
    "seo:sitemap": "next build && node -e \"console.log('Sitemap generated at /sitemap.xml')\"",
    "analyze": "cross-env ANALYZE=true next build"
  },
